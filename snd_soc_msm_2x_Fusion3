	Comment "Modifier for music playback"

	EnableSequence
		'SLIMBUS_0_RX Audio Mixer MultiMedia1':1:1
	EndSequence

	DisableSequence
		'SLIMBUS_0_RX Audio Mixer MultiMedia1':1:0
	EndSequence

	# ALSA PCMs
	CapturePCM 0
	PlaybackPCM 0

EndSection

SectionModifier
	Name "Play MusicSpeaker FM Tx"
