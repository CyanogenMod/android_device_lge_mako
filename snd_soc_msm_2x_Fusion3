SectionDevice
	Name "Earpiece"
	Comment "Handset Rx device"

	EnableSequence
		'SLIM_0_RX Channels':0:One
		'RX1 MIX1 INP1':0:RX1
		'RX1 MIX1 INP2':0:RX6
		'EAR PA Gain':0:POS_2_DB
		'DAC1 Switch':1:1
		'SLIM TX3 MUX':0:RMIX1
	EndSequence

	DisableSequence
		'RX1 MIX1 INP1':0:ZERO
		'RX1 MIX1 INP2':0:ZERO
		'DAC1 Switch':1:0
		'SLIM TX3 MUX':0:ZERO
	EndSequence

	ACDBID	7:1
	EffectsMixerCTL "SRS TruMedia"

EndSection

